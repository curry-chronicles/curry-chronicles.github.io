{
	"compileOnSave": false,
	"compilerOptions": {
		"baseUrl": "./",
		"declaration": false,
		"downlevelIteration": true,
		"esModuleInterop": true,
		"experimentalDecorators": true,
		"importHelpers": true,
		"module": "es2020",
		"moduleResolution": "node",
		"outDir": "./dist/out-tsc",
		"paths": {
			"@curry-chronicles/models": [
				"./src/app/models/index"
			],
			"@curry-chronicles/infra": [
				"./src/app/infra/index"
			]
		},
		"sourceMap": true,
		"target": "es2015",
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2018",
			"dom"
		],
		"resolveJsonModule": true
	},
	"angularCompilerOptions": {
		"fullTemplateTypeCheck": true,
		"strictInjectionParameters": true
	}
}